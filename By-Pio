withdraw.Text = " Withdraw "
withdraw.TextColor3 = Color3.fromRGB(255, 255, 255)
withdraw.TextSize = 14.000
withdraw.MouseButton1Down:connect(function()
	local args = {
		[1] = {
			[1] = "9db6d62a-32eb-4429-9e39-9193dd189b30",
			[2] = {},
			[3] = 25000000000
		}
	}

	workspace.__THINGS.__REMOTES:FindFirstChild("bank withdraw"):InvokeServer(unpack(args))
end)

